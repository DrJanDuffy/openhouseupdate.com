{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build:fast": "cross-env NODE_ENV=production qwik build --minify",
    "build:analyze": "cross-env NODE_ENV=production qwik build --analyze",
    "build:clean": "rm -rf dist tmp .tsbuildinfo && pnpm build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit --skipLibCheck",
    "build.types:watch": "tsc --incremental --noEmit --skipLibCheck --watch",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr",
    "dev:fast": "vite --mode ssr --force",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "biome format --write .",
    "fmt.check": "biome format .",
    "lint": "biome lint .",
    "lint.fix": "biome lint --write .",
    "lint.unsafe": "biome lint --write --unsafe .",
    "check": "biome check .",
    "check.fix": "biome check --write .",
    "check.unsafe": "biome check --write --unsafe .",
    "check.ci": "biome ci",
    "check.errors": "biome check --diagnostic-level=error .",
    "check.changed": "biome check --changed .",
    "check.staged": "biome check --staged .",
    "preview": "qwik build preview && vite preview --open",
    "git:optimize": "powershell -ExecutionPolicy Bypass -File scripts/optimize-git.ps1",
    "git:status": "git status --porcelain",
    "git:push": "git push origin main --verbose",
    "git:commit": "git add . && git commit -m",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "cache:clear": "rm -rf node_modules/.cache tmp .tsbuildinfo",
    "cache:clean": "pnpm store prune && pnpm cache:clear",
    "deps:update": "pnpm update --interactive --latest",
    "deps:audit": "pnpm audit --audit-level moderate"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@builder.io/qwik": "1.0.0",
    "@builder.io/qwik-city": "~1.0.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/eslint": "8.37.0",
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "autoprefixer": "^10.4.14",
    "cross-env": "^10.0.0",
    "esbuild": "^0.25.10",
    "eslint": "8.39.0",
    "eslint-plugin-qwik": "0.104.0",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.3.1",
    "typescript": "5.0.4",
    "undici": "5.22.0",
    "vercel": "^28.19.0",
    "vite": "4.3.2",
    "vite-bundle-analyzer": "^1.2.3",
    "vite-tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0"
  }
}
