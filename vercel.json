{
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000, s-maxage=2592000"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(self)"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/home-valuation",
      "destination": "/services/home-valuation",
      "permanent": true
    },
    {
      "source": "/buyer-services",
      "destination": "/services/buyer-services",
      "permanent": true
    },
    {
      "source": "/seller-services",
      "destination": "/services/seller-services",
      "permanent": true
    },
    {
      "source": "/market-analysis",
      "destination": "/services/market-analysis",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/sitemap-images.xml",
      "destination": "/sitemap-images.xml"
    },
    {
      "source": "/sitemap-news.xml",
      "destination": "/sitemap-news.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/rss.xml",
      "destination": "/rss.xml"
    }
  ],
  "crons": [
    {
      "path": "/api/sitemap-update",
      "schedule": "0 2 * * *"
    }
  ]
}