{
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000, s-maxage=2592000"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(self)"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.openhouseupdate.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "openhouseupdate.com"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "destination": "https://www.openhouseupdate.com/robots.txt",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "openhouseupdate.com"
        }
      ]
    },
    {
      "source": "/home-valuation",
      "destination": "/services/home-valuation",
      "permanent": true
    },
    {
      "source": "/buyer-services",
      "destination": "/services/buyer-services",
      "permanent": true
    },
    {
      "source": "/seller-services",
      "destination": "/services/seller-services",
      "permanent": true
    },
    {
      "source": "/market-analysis",
      "destination": "/services/market-analysis",
      "permanent": true
    },
    {
      "source": "/about",
      "destination": "/about/",
      "permanent": true
    }
  ],
  "crons": [
    {
      "path": "/api/sitemap-update",
      "schedule": "0 2 * * *"
    }
  ]
}
